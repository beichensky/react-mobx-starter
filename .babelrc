{
    "presets": [
        "env",
        "stage-0",
        "react"
    ],
    "plugins": [
        "transform-decorators-legacy",
        [
            "import",
            {
                "libraryName": "antd",
                "style": true
            }
        ],
        "transform-runtime",
        "transform-class-properties"
    ]
}